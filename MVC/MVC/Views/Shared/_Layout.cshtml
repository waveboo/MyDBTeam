<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Task</title>
    <link href="@Url.Content("http://apps.bdimg.com/libs/bootstrap/3.3.4/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("http://cdn.gbtags.com/jquery/2.1.1/jquery.min.js")" type="text/javascript" charset="utf-8"></script>
    <script src="@Url.Content("http://apps.bdimg.com/libs/bootstrap/3.3.4/js/bootstrap.min.js")" type="text/javascript" charset="utf-8"></script>
    <link rel="stylesheet" href="@Url.Content("~/Content/background.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/Button.css")" />
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                @Html.ActionLink("TJSSESU", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-gbtag" aria-expanded="false">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="collapse navbar-collapse" id="navbar-gbtag">

                <ul id="navn" class="nav navbar-nav">

                    <li>@Html.ActionLink("组织构架", "Organization", "Home")</li>
                    <li>@Html.ActionLink("成员管理", "Member", "Home")</li>
                    <li class="active">
                        @Html.ActionLink("新闻版块", "Index", "Home")
                    </li>
                    <li>@Html.ActionLink("权益反馈", "Feedback", "Home")</li>
                    <li>@Html.ActionLink("文件管理", "About", "Home")</li>
                    <li>@Html.ActionLink("任务管理", "Task", "Home")</li>
                    <li class="dropdown">
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    @RenderBody()
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="@Url.Content("~/Scripts/Layout.js")" type="text/javascript" charset="utf-8"></script>
</body>
</html>
